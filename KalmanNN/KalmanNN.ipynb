    "def max_len_coordinate(dset, keys):\n",
    "    max_size = 0\n",
    "    for key_k in keys:\n",
    "        size = len(np.array(dset[key_k])[:,0])\n",
    "        if size > max_size:\n",
    "            max_size = size\n",
    "            key = key_k\n",
    "    return (max_size, key)\n",
    "        "
